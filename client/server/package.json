{
  "name": "raa",
  "version": "1.0.12",
  "description": "RAA Server",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "SET NODE_ENV=development&& nodemon -r dotenv/config ./bin/www",
    "server-prod": "NODE_ENV=development&& nodemon ./bin/www",
    "prod": "NODE_ENV=production&& nodemon -r dotenv/config ./bin/www",
    "client": "cd client && npm install && npm run build",
    "linux": "cd client && npm install && npm run build",
    "dev": "node --inspect app.js"
  },
  "author": "RAA - Development team",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^1.2.2",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mysql2": "^2.1.0",
    "react": "^16.13.1",
    "redis": "^4.5.1",
    "sequelize": "^5.21.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
